<div class="container">
  <%= simple_form_for [ @enquiry ] do |f| %>
    <%= f.input :title %>
    <%= f.input :property_address %>
    <%= f.input :information %>
    <%= f.input :date, html5: true %>
    <%= f.input :timeslot, collection: ['AM', 'PM', 'All Day']%>
    <%= f.hidden_field "contractor_id", value: @contractor.id %>
    <%= f.submit %>
  <% end %>
</div>
